{
  "name": "mini",
  "version": "1.0.0",
  "description": "mini frontend framework",
  "main": "main.js",
  "source": "index.html",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "start": "npm run transpile | npm run serve",
    "transpile": "nodemon --watch './*' --watch './pages/*' --watch './mini/*' --exec 'node esbuild.config.js && echo reload'",
    "serve": "lite-server --port 2505",
    "build": "node esbuild.config.js"
  },
  "author": "mohammed hrima",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "browser-sync": "^3.0.2",
    "esbuild": "0.20.1",
    "http-server": "^14.1.1",
    "lite-server": "^2.6.1",
    "nodemon": "^3.1.0",
    "parcel": "latest",
    "parcel-bundler": "^1.12.5"
  }
}