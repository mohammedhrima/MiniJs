{
  "jsc": {
    "target": "es5",
    "parser": {
      "syntax": "typescript",
      "tsx": true
    }
  },
  "bundle": {
    "entry": "./src/main.js",
    "outDir": "./static",
    "loader": {
      ".js": "jsx"
    },
    "jsxFactory": "Mini.createElement",
    "jsxFragment": "Mini.Fragment",
    "plugins": [
      {
        "name": "jpg-loader",
        "setup": "swc-jpg-loader.js"
      }
    ]
  }
}
